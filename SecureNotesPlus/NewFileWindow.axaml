<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight=""
        Width="400"
        Height="160"
        WindowStartupLocation="CenterScreen"
        x:Class="SecureNotesPlus.NewFileWindow"
        Title="NewFile">
        
        <StackPanel>
                <TextBox Margin="10" Name="NameFileBox" Watermark="Name note"></TextBox>
                <StackPanel Orientation="Horizontal">
                        <Button Margin="10" Name="CreateNoteButton" Click="CreateNoteButton_OnClick" Content="Create note" ></Button>
                </StackPanel>
        </StackPanel>
        
</Window>
